{{#if products}}
    <table>
        <thead>
            <th>Title</th>
            <th>Price</th>
            <th>Photo</th>
        </thead>
        <tbody>
            {{#each products}}
                <tr>
                    <td>{{this.title}}</td>
                    <td>{{this.price}}</td>
                    <td><img width="100px" src="{{this.photo}}"/></td>
                </tr>
            {{/each}}
        </tbody>
    </table>
{{else}}
<p>No hay productos</p>
{{/if}}